noauth refuse-eap
user "L2TP_USERNAME"
password "L2TP_PASSWORD"
nomppe nomppc
maxfail 0
usepeerdns
L2TP_PERSIST_DEMAND
defaultroute
ipcp-accept-remote L2TP_LOCAL_IPADDR noipdefault
ktune
default-asyncmap nopcomp noaccomp
novj nobsdcomp nodeflate
lcp-echo-interval 5
lcp-echo-failure 5
unit L2TP_UNITID
ipparam IFACE

}

div.sidenav {
     width: 230px;
}

.nav {
    margin: 0 auto;
    padding: 0;
    height: 40px;
    background: #000000;
	border-top: 1px solid #666666;
	border-bottom: 0px solid #666666;
	float: left;
	overflow: hidden;
	position: relative;
}

.nav ul {
	width: 1024px;
	clear: left;
	float: left;
	list-style: none;
	margin: 0 auto;
	padding: 0;
	left: 50%;
	text-align: center;
}

.nav ul li {
	width: 256px;
	height: auto;
	display: inline;
	float: left;
	list-style-type:none;
	margin: 0;
	padding: 0;
	right: 50%;
}

.nav li .menu-item {
    height: 50px;
    padding: 0 0px;
    color: #FFF; 
    font-weight: normal;
    font-size: 12px;
    line-height: 40px;
}

.nav li a {
	width: 256px;
    font-size: 12px;
    padding-top: 0px;
    float: left;
    color: #FFF; 
    text-decoration: none;
    font-size: 12px;
    vertical-align: middle;
}

.nav li a:link {
	background: transparent url(images/menu-off.png) no-repeat 0pt;
	background-size: contain;
}

.nav li a:hover
 {
	color: #00FFFF
}

.nav li a:active {
	color: #008080
}

.nav ul li .menu-item:hover {
}

.nav li:hover .sub {
    display: block
}

.nav ul li .sub {
/*  background: #666666;*/
    display: none; 
    width: 128px; 
    position: absolute;
    left: 0px;
    top: 0px;
    z-index: 10;
}

.nav ul li .sub li {
	padding-top: 5px;
    padding-left: 0px;
    /*background: #000000 url(images/item.png) no-repeat;*/
    background: #000000;
    /*border: 2.5px solid;
    line-height: 20px;*/
    height: auto;
}

.nav li ul {
    width: 1024px; 
    height: auto
}

.nav li:hover li {
    /*margin-bottom: 0.5px;*/
    width: 1024px;
}

.nav li:hover li a {
    padding-top: 0px; 
    padding-right: 0px; 
    padding-left: 0px; 
    padding-bottom: 0px;         
    text-align: center;
    color: white; 
    text-indent: 1px; 
    line-height: normal; 
}

.sub li .menu-subitem {
    margin-left: 1px;
    margin-bottom: 7px;
}


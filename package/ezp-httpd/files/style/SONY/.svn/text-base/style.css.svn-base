/*.* custom stuff */

html {
  height: 120%;
  margin-bottom: 1px;
}
fieldset {
    border: 2px;
    border-color: #999999;
    border-style: solid;
    padding: 10px;
    margin: 5px;
    width: auto;
}
.setting {
    line-height: 2.3em;
    width: auto;
    font-size: 12px;
    postition: relative;
    margin-left: 10px;
    margin-right: 10px;
}
fieldset .setting {
    line-height: 2.3em;
    width: auto;
    font-size: 12px;
    postition: relative;
    margin-left: 10px;
    margin-right: 10px;
}
.input-window fieldset .setting {
    line-height: 1.7em;
}
#controls {
    font-size: 12px;
}
.label,
fieldset .setting .label {
    float: left;
    width: 290px;
    color: #000000;
}
fieldset .setting .ilabel {
    float: left;
    width: 250px;
    color: #000000;
}
fieldset legend {
    font-size: 12px;
    font-weight: bold;
    padding-left: 0.2em;
    padding-right: 0.2em;
    margin-left: 0.5em;
    color: #000000;
}
textarea {
    margin-left: 20px;
    font-size: 12px;
    font-family: arial, helvetica, sans-serif;
}
input {
    font-size: 12px;
    font-family: arial, helvetica, sans-serif;
}
select {
    font-size: 12px;
    font-family: arial, helvetica, sans-serif;
}
#contentsInfo {
    padding: 5px;
    width: auto;
}
/* small column */
#contentsInfo div.scol2l {
    float: left; 
    width: 260px
}
#contentsInfo div.scol2r {
    float: left; 
    margin-left: 6px;
    width: 260px
}
/* large column */
#contentsInfo div.lcol2l {
    float: left; 
    width: 480px
}
#contentsInfo div.lcol2l .setting .label {
    width: 140px;
}
#contentsInfo div.lcol2r {
    float: left; 
    margin-left: 6px;
    width: 480px
}
#contentsInfo div.lcol2r .setting .label {
    width: 140px;
}
body {
